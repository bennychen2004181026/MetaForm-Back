{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "endOfLine": "off"
  }
}
